            <connector>
                <targetReference>Second_Trade_Task_Creation</targetReference>
            </connector>
            <label>Second Trade Record Type?</label>
        </rules>
    </decisions>
    <description>To Create Payment Processing Request from the opportunity record through button</description>
    <environments>Default</environments>
    <formulas>
        <name>forT_Description</name>
        <dataType>String</dataType>
        <expression>&quot;Set up the payments for &quot; &amp;{!Get_Opportunity_Record.Account.FirstName} &amp; &quot; &quot;&amp; {!Get_Opportunity_Record.Account.LastName}</expression>
    </formulas>
